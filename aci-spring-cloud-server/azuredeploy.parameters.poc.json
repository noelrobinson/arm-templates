{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "containerName": {
            "value": "spring-cloud-config"
        },
        "location": {
            "value": "uksouth"
        },
        "imageType": {
            "value": "Public"
        },
        "imageName": {
            "value": "hyness/spring-cloud-config-server"
        },
        "osType": {
            "value": "Linux"
        },
        "numberCpuCores": {
            "value": "1"
        },
        "memory": {
            "value": "1.5"
        },
        "restartPolicy": {
            "value": "OnFailure"
        },
        "springProfileActive": {
            "value": "security"
        },
        "springSecurityUserName": {
            "value": "cloudconfig"
        },
        "springSecurityPassword": {
            "value": null
        },
        "configServerGitUrl": {
            "value": "ssh://git@bitbucket.org:nationaltrust/{application}-config.git"
        },
        "ignoreLocalSshSetting": {
            "value": "true"
        },
        "hostKeyChecking": {
            "value": "false"
        },
        "gitPrivateKey": {
            "value": null
        },
        "ipAddressType": {
            "value": "Public"
        },
        "ports": {
            "value": [
                {
                    "port": "8888",
                    "protocol": "TCP"
                }
            ]
        },
        "dnsNameLabel": {
            "value": "tribelo"
        }
    }
}